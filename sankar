from subprocess import check_output
from pathlib import Path
import csv
import re

cmd_out = []

def command(args):
    out = check_output(str(args).split()).decode()
    cmd_out.append(out.strip())
            
command('hostname')
command('hostname -i')

file = Path('/home/mana/Downloads/putty.log').read_text()
match = re.search(r'midtier*.*\n \nBLRAPC02',file,re.S).group().splitlines()[5:-2]
match = '\n'.join(match)
cmd_out.append(match)
with open('/home/mana/Work/sankar.csv','w') as f:
    writer = csv.writer(f)
    writer.writerow(cmd_out)
